import java.text.NumberFormat;
import java.util.Scanner;

// FizzBuzz Program
// Get inputs from User : Number, and print Fizz if the number is divisible by 5, print Buzz if divisible by 3, print FizzBuzz if divisible by both 5 and 3. Print the number if not divisible by 5 or 3.
// Tips: Use Modulo operator to get the remainder of the Division operation. 2. Order the If clause from most specific case to generic case.
public class Main {
    public static void main(String[] args) {
        System.out.println("FizzBuzz Program");

        Scanner scanner = new Scanner(System.in);

        System.out.print("Number: ");
        int Number = scanner.nextInt();

        if (Number % 5 == 0 && Number % 3 == 0)
            System.out.println("FizzBuzz");
        else if (Number % 3 == 0)
            System.out.println("Buzz");
        else if (Number % 5 == 0)
            System.out.println("Fizz");
        else
            System.out.println(Number);
    }
}
